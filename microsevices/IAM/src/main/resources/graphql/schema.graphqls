type Mutation {
    ajouterFavoris(idEtd: ID!, idStage: ID!): String
    supprimerFavoris(idEtd: ID!, idStage: ID!): String
}

type Query {
    etudiantById(id: ID!): Etudiant
}

type Etudiant {
    id: ID!
    nom: String!
    prenom: String!
    email: String!
    filiere: String
    type: String
    favoris: [ID!]  # Liste d'IDs pour les favoris
}